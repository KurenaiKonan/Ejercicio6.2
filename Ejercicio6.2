        self._nombre = nombre
        self._nota = nota

    def resultado( self ) :
        return f'{ self._nombre } con una nota de { self._nota } { "ha aprobado" if self._nota >= self._nota_minima else f"NO aprob√≥, la nota minima es de { self._nota_minima }" } '

Jace = Alumno( 'Jace', 2 )
print( jace.resultado() )

